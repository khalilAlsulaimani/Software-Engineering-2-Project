<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AppCode.LogInScreenController">
   <children>
      <TextField fx:id="usernameInput" layoutX="226.0" layoutY="174.0" onAction="#signInButton" promptText="Enter Username" />
      <Label layoutX="247.0" layoutY="30.0" text="Sign In ">
         <font>
            <Font size="33.0" />
         </font>
      </Label>
      <TextField fx:id="passwordInput" layoutX="226.0" layoutY="244.0" onAction="#signInButton" promptText="Enter Password" />
      <Label layoutX="132.0" layoutY="178.0" text="Username :" />
      <Label layoutX="134.0" layoutY="248.0" text="Password :" />
      <Label fx:id="errorLabel" alignment="CENTER" layoutX="69.0" layoutY="109.0" prefHeight="27.0" prefWidth="461.0" textFill="RED">
         <font>
            <Font name="Constantia" size="18.0" />
         </font></Label>
      <Button defaultButton="true" layoutX="219.0" layoutY="323.0" mnemonicParsing="false" onAction="#signInButton" text="Sign In" />
      <Button defaultButton="true" layoutX="329.0" layoutY="323.0" mnemonicParsing="false" onAction="#clearSignIn" text="Clear" />
   </children>
</AnchorPane>
